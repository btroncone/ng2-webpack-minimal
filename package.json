{
  "name": "ng2-webpack-minimal",
  "version": "0.0.1",
  "description": "Stripped down version of @AngularClass Webpack Starter",
  "main": "",
  "scripts": {
    "build": "npm run webpack --colors --display-error-details --display-cached",
    "webpack": "webpack",
    "clean": "rimraf node_modules doc",
    "clean-install": "npm run clean && npm install && tsd install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch",
    "server": "webpack-dev-server --inline --colors --display-error-details --display-cached --port 3000",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/btroncone/ng2-webpack-minimal"
  },
  "author": "btroncone@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@reactivex/rxjs": "5.0.0-alpha.7",
    "angular2": "2.0.0-alpha.47",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "ng2-redux": "^2.0.0",
    "redux": "^3.0.4",
    "redux-devtools": "^2.1.5",
    "redux-thunk": "^1.0.0",
    "reflect-metadata": "0.1.2",
    "zone.js": "0.5.8"
  },
  "devDependencies": {
    "babel": "^5.8.12",
    "babel-loader": "^5.3.2",
    "babel-plugin-angular2-annotations": "^2.0.1",
    "css-loader": "^0.21.0",
    "exports-loader": "0.6.2",
    "expose-loader": "0.7.0",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.4",
    "json-loader": "^0.5.3",
    "object-assign": "4.0.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.4.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
